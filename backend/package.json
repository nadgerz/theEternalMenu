  "homepage": "https://github.com/MoppetX/theEternalMenu#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "config": "^3.2.4",
    "core-js": "^3.4.5",
    "draft-js": "^0.11.2",
    "express": "^4.17.1",
    "lint-staged": "^9.5.0",
    "mongoose": "^5.7.13",
    "regenerator-runtime": "^0.13.3",
    "uuid": "^3.3.3",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
