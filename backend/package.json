    "mongodb-memory-server": "^6.0.1",
    "nodemon": "^2.0.1",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
