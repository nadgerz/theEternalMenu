{
  "root": true,
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-console": 1,
    "no-unused-vars": "error"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
